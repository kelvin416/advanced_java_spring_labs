<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="greenFiction" class="platform.codingnomads.co.corespring.lab.ScienceFiction">
        <property name="movie" value="Green Lantern"/>
        <property name="year" value="2011"/>
    </bean>

    <bean id="superFiction" class="platform.codingnomads.co.corespring.lab.ScienceFiction">
        <property name="movie" value="Super Man"/>
        <property name="year" value="2007"/>
    </bean>

    <bean id="spiderFiction" class="platform.codingnomads.co.corespring.lab.ScienceFiction">
        <property name="movie" value="Spider Man"/>
        <property name="year" value="2003"/>
    </bean>
</beans>

